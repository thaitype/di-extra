{
  "name": "@di-extra/mono",
  "version": "0.0.1",
  "description": "tRPC Azure Functions Adapter",
  "main": "./dist/main.js",
  "module": "./dist/main.mjs",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.mjs",
      "default": "./dist/main.js"
    }
  },
  "files": [
    "dist/**",
    "src/**"
  ],
  "scripts": {
    "test": "tsx ./scripts/test-runner.ts",
    "build": "nx run-many -t build",
    "build:all": "nx run-many -t all",
    "all": "run-s test build:all"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "ava": "^5.1.1",
    "c8": "^7.12.0",
    "esbuild": "^0.19.11",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.56.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "nx": "^17.2.8",
    "reflect-metadata": "^0.2.1",
    "tiny-glob": "^0.2.9",
    "tsup": "^6.6.3",
    "tsx": "^4.7.0",
    "typescript": "^5.0.2"
  },
  "keywords": [],
  "author": "Thada Wangthammang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thaitype/inversify.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "esbuild-register"
    ]
  }
}