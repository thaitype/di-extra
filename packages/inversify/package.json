{
  "name": "@di-extra/inversify",
  "version": "0.1.0",
  "description": "Type-safe Dependency Injection Wrapper for inversify",
  "main": "./dist/main.js",
  "module": "./dist/main.mjs",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.mjs",
      "default": "./dist/main.js"
    }
  },
  "scripts": {
    "all": "run-s  build lint",
    "build": "tsup src/main.ts --dts --format esm,cjs",
    "lint": "tsc --noEmit",
    "test": "ava",
    "dev": "nodemon --watch src --ext ts --exec 'npm run build'"
  },
  "files": [
    "dist/**",
    "src/**"
  ],
  "keywords": [
    "inversify",
    "dependency-injection",
    "typescript"
  ],
  "author": "Thada Wangthammang",
  "license": "MIT",
  "dependencies": {
    "inversify": "^6.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thaitype/inversify.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}